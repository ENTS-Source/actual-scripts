{
  "name": "actual-scripts",
  "private": true,
  "version": "0.1.0",
  "description": "Import and reporting scripts for Actual Budget",
  "main": "lib/index.ts",
  "scripts": {
    "build": "tsc",

    "sync": "npm run-script build && node dist/index.js sync",
    "import": "npm run-script build && node dist/index.js import",

    "start": "npm run-script build && node dist/index.js",
    "statement:balance": "npm run-script build && node dist/statements/balance.js",
    "statement:income": "npm run-script build && node dist/statements/income.js",
    "statement:spending": "npm run-script build && node dist/statements/spending.js"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@actual-app/api": "^25.3.1",
    "commander": "^13.1.0",
    "csv-parse": "^5.6.0",
    "exceljs": "^4.4.0",
    "moment": "^2.30.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "typescript": "^5.8.2"
  }
}
